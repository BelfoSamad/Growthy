{
	"name": "Youtube Extension Prototype",
	"version": "1.0",
	"description": "Trying.. lol",
	"manifest_version": 2,
	"permissions": [
          "tabs"
        ],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
    },
    "content_scripts": [
        {
        "matches": [
            "http://www.youtube.com/*",
            "https://www.youtube.com/*"
            ],
        "js": ["content.js"],
        "run_at": "document_end"
        }
    ]
}